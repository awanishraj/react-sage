{
  "name": "react-sage",
  "version": "0.0.50",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prebuild": "npm run clean && npm run lint && npm run prettier",
    "build": "tsc",
    "clean": "rm -rf dist",
    "demo": "parcel demo/index.html --out-dir demo/output --no-cache --no-autoinstall",
    "build:demo": "parcel build demo/index.html --out-dir demo/output --no-cache --no-autoinstall",
    "lint": "tsc --noEmit && eslint 'src/*/**/*.{ts,tsx}' --quiet --fix",
    "prepublish": "npm run build",
    "prettier": "prettier -l 'src/**/*.ts*'",
    "prettier:fix": "npm run prettier -- --format",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meinstein/react-sage.git"
  },
  "keywords": [
    "react",
    "hooks",
    "form",
    "query",
    "mutation",
    "file",
    "input",
    "storage"
  ],
  "author": "Maximillian Einstein",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.26",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-jest": "^25.3.0",
    "eslint": "^7.1.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jest": "^25.3.0",
    "parcel": "^1.12.4",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {},
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
